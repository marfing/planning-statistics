{% extends 'base.html.twig' %}

{% block title %} CSV values loaded {% endblock %}

{% block body %}
    <h1>Deleted csv files from directory: {{ path }}</h1>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>Number of deleted CSV files</th>
                <th>Number of wrong type files</th>
            </tr>
        </thead>
        <tbody>
            <tr class="table-info">
                <td>{{ phpFiles }}</td>
                <td>{{ wrongFiles }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('network_element_index') }}">back to list</a>
{% endblock %}